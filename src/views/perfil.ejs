        </div>

          <hr>

          <h2>
            <% try { %>
              <%= usuario[0].Usuario %>
              <h6>(<%= usuario[0].Nome %>)</h6>
            <% } catch (error) { %>
              erro
            <% } %>
          </h2>

          <br>

          <div class="p-0 d-flex flex-column" id="informacoes">
            <% try { %>
              <% usuario[0].Usuario %>
              <p>Email: <%= usuario[0].Email %></p>

              <hr>

              <div class="d-flex justify-content-center align-items-center flex-column" id="bttnsUsuario">
                <button class="btnUsuario" data-bs-toggle="modal" data-bs-target="#atualizarPerfil" style="background: rgb(73, 73, 73); border: black 1px solid; width: 100%;">Editar Perfil</button>

                <br>

                <button class="btnUsuario" data-bs-toggle="modal" data-bs-target="#logout" style="background: transparent; border: none; width: fit-content;"><i class="fa-solid fa-right-to-bracket fa-xl" style="color: #ff0101;"></i></button>
              </div>
            <% } catch (error) { %>
              <p>ERRO</p>
            <% } %>
          </div>

        </div>

      </div>

      <div class="container d-flex flex-column" id="listasContainer">

        <div class="d-flex flex-row">
          <div class="container d-flex align-items-end justify-content-center p-0" id="bttnListaFilmes">
            <button id="botaoListaFilmes" class="btn btn-primary d-flex align-items-end justify-content-center fs-2" type="button" data-bs-toggle="collapse" data-bs-target="#listaFilmes" aria-expanded="false" aria-controls="collapseExample" onclick="listaFilmesClick()">
              Lista de Filmes
            </button>
          </div>

          <div class="container d-flex align-items-end justify-content-center p-0" id="bttnListaSeries">
            <button id="botaoListaSeries" class="btn btn-outline-primary d-flex align-items-end justify-content-center fs-2" type="button" data-bs-toggle="collapse" data-bs-target="#listaSeries" aria-expanded="false" aria-controls="collapseExample" onclick="listaSeriesClick()">
              Lista de Series
            </button>
          </div>
        </div>
          
          
          <div class="container p-0 overflow-y-auto d-flex flex-column" id="lista">
            
            <div class="collapse show" id="listaFilmes">

              <div id="btnsLista">
                <button id="botaoAssistidosFilmes" class="btn btn-primary" type="button" data-bs-toggle="collapse" data-bs-target="#filmesAssistidos" aria-expanded="false" aria-controls="collapseExample" onclick="listaFilmesAssistidosClick()" style="background-color: transparent; border: none;">
                  Assistidos
                </button>
                <button id="botaoPretendoAssistirFilmes" class="btn btn-outline-primary" type="button" data-bs-toggle="collapse" data-bs-target="#filmesPretendoAssistir" aria-expanded="false" aria-controls="collapseExample" onclick="listaFilmesPretendoAssistirClick()" style="background-color: transparent; border: none;">
                  Pretendo Assistir
                </button>
              </div>
              
              <div class="collapse show" id="filmesAssistidos">
                <% try { %>
                  <% for(let i = 0; i < listaFilmes.length; i++) { %>
                    
                    <div class="filme d-flex flex-row">
                      <a href="/filmeSelec/<%= listaFilmes[i].IDFilme %>/<%= usuario[0].Usuario %>">
                        <img  class="imagemFilme" src="/img/<%= filmes[i]['filme.Imagem'] %>" alt="Imagem n√£o encontrada">
                      </a>
                      
                      <div class="infoFilme d-flex flex-column justify-content-between">
                        <div class="d-flex flex-row justify-content-between align-items-center">
                          <h2><%= filmes[i]['filme.Titulo'] %></h2>
                          <form action="/favoritarFilme/<%= listaFilmes[i].IDFilme %>/<%= usuario[0].Usuario %>" method="post">
                            <% if (listaFilmes[i].Favorito == 0) { %>
                              <button type="submit" style="border: none; background-color: transparent;">
                                <i class="heart fa-regular fa-heart fa-xl" style="color: #ff9c00;" id="heart<%=i%>"onclick="mudarFav('heart<%= i%>', 0)"></i>
                              </button>
                            <% } else { %>
                              <button type="submit" style="border: none; background-color: transparent;">
                                <i class="heart fa-solid fa-heart fa-xl" style="color: #ff9c00;" id="heart<%=i%>"onclick="mudarFav('heart<%= i%>', 1)"></i>
                              </button>
                            <% } %>
                          </form>
                        </div>
                        <hr style="position: relative; bottom: 40px;">
                        

                        <div style="position: relative; bottom: 40px;">

                          <p>
                            <%= listaFilmes[i].Comentario %>
                          </p>
                          
                        </div>
                        
                        <div class="d-flex justify-content-between align-items-end gap-1">
                          <% for(let j = 0; j < 5; j++) { %>
                            <i class="fa fa-star" <%= (j < listaFilmes[i].Nota) ? 'style=color:#FC0' : "" %>></i>
                            <% } %>
                            
                            <div class="d-flex justify-content-end align-items-end" id="btnsEdit">
                              <button id="editReview" class="d-flex justify-content-center" data-bs-toggle="modal" data-bs-target="#editarReviewFilme<%=i%>" style="background: transparent; border: none;"><i class="fa-regular fa-pen-to-square fa-xl" ></i></button>
                              <button class="d-flex justify-content-center" data-bs-toggle="modal" data-bs-target="#excluirReviewFilme<%=i%>" style="background: transparent; border: none; color: red;"><i class="fa-solid fa-trash fa-xl"></i></button>
                            </div>
                            
                          </div>
                          
                        </div>
                        
                      </div>

                      <hr>
                      <% } %>
                      <% } catch (error) { %>
                        <% console.log(error) %>
                        <p>Lista Vazia!</p>
                        <% } %>
                </div>

                <div class="collapse" id="filmesPretendoAssistir">
                  <% try { %>
                    <% for(let i = 0; i < listaPretendeAssistirFilme.length; i++) { %>
                      
                      <div class="filme d-flex flex-row">
                        <a href="/filmeSelec/<%= listaPretendeAssistirFilme[i].IDFilme %>/<%= usuario[0].Usuario %>">
                          <img  class="imagemFilme" src="/img/<%= listaPretendeAssistirFilme[i]['filme.Imagem'] %>" alt="Imagem n√£o encontrada">
                        </a>
                        
                        <div class="infoFilme d-flex flex-column justify-content-between">
                          <div>
                            <h2><%= listaPretendeAssistirFilme[i]['filme.Titulo'] %></h2>
                            <hr>
                          </div>
                            
                        </div>
                          
                        </div>
                        <% } %>
                        <% } catch (error) { %>
                          <% console.log(error) %>
                          <p>Lista Vazia!</p>
                          <% } %>
                  </div>
            </div>


            <div class="collapse" id="listaSeries">

              <div id="btnsLista">
                <button id="botaoAssistidosSeries" class="btn btn-primary" type="button" data-bs-toggle="collapse" data-bs-target="#seriesAssistidos" aria-expanded="false" aria-controls="collapseExample" onclick="listaSeriesAssistidosClick()" style="background-color: transparent; border: none;">
                  Assistidos
                </button>
                <button id="botaoPretendoAssistirSeries" class="btn btn-outline-primary" type="button" data-bs-toggle="collapse" data-bs-target="#seriesPretendoAssistir" aria-expanded="false" aria-controls="collapseExample" onclick="listaSeriesPretendoAssistirClick()" style="background-color: transparent; border: none;">
                  Pretendo Assistir
                </button>
              </div>
              
              <div class="collapse show" id="seriesAssistidos">
                <% try { %>
                  <% for(let i = 0; i < listaSeries.length; i++) { %>
                    
                    <div class="filme d-flex flex-row">
                      <a href="/serieSelec/<%= listaSeries[i].IDSerie %>/<%= usuario[0].Usuario %>">
                        <img  class="imagemFilme" src="/img/<%= series[i]['serie.Imagem'] %>" alt="Imagem n√£o encontrada">
                      </a>
                      
                      <div class="infoFilme d-flex flex-column justify-content-between">
                        <div>
                          <h2><%= series[i]['serie.Titulo'] %></h2>
                          <hr>
                        </div>
                        
                        <div>
                          
                          <p>
                            <%= listaSeries[i].Comentario %>
                            <%= series[i]['serie.NotaGeral'].toFixed(1) %>
                          </p>
                          
                        </div>
                        
                        <div class="d-flex justify-content-between align-items-end gap-1">
                          <% for(let j = 0; j < 5; j++) { %>
                            <i class="fa fa-star" <%= (j < listaSeries[i].Nota) ? 'style=color:#FC0' : "" %>></i>
                            <% } %>
                            
                            <div class="d-flex justify-content-end" id="btnsEdit">
                              <button class="d-flex justify-content-center" data-bs-toggle="modal" data-bs-target="#editarReviewSerie<%=i%>" style="background: transparent; border: none;"><span class="material-symbols-outlined">edit_note</span></button>
                              <button class="d-flex justify-content-center" data-bs-toggle="modal" data-bs-target="#excluirReviewSerie<%=i%>" style="background: transparent; border: none; color: red;"><span class="material-symbols-outlined">delete</span></button>
                            </div>
                            
                          </div>
                          
                        </div>
                        
                      </div>
                      <% } %>
                      <% } catch (error) { %>
                        <% console.log(error) %>
                        <p>Lista Vazia!</p>
                        <% } %>
                </div>

                <div class="collapse" id="seriesPretendoAssistir">
                  <% try { %>
                    <% for(let i = 0; i < listaPretendeAssistirSerie.length; i++) { %>
                      
                      <div class="filme d-flex flex-row">
                        <a href="/serieSelec/<%= listaPretendeAssistirSerie[i].IDSerie %>/<%= usuario[0].Usuario %>">
                          <img  class="imagemFilme" src="/img/<%= listaPretendeAssistirSerie[i]['serie.Imagem'] %>" alt="Imagem n√£o encontrada">
                        </a>
                        
                        <div class="infoFilme d-flex flex-column justify-content-between">
                          <div>
                            <h2><%= listaPretendeAssistirSerie[i]['serie.Titulo'] %></h2>
                            <hr>
                          </div>
                            
                          </div>
                          
                        </div>
                        <% } %>
                        <% } catch (error) { %>
                          <p>Lista Vazia!</p>
                          <% } %>
                  </div>
            </div>
          </div>

      </div>

    
    </main>


    <footer class="d-flex">

      <div class="container d-flex flex-row justify-content-evenly mb-3 flex-wrap">
        <div>
          <i class="fa-brands fa-square-github" style="color: #ff9c00;"></i> @AmiltonMn
        </div>
        <div>
          <i class="fa-solid fa-envelope" style="color: #ff9c00;"></i>amilton.moreiran@hotmail.com
        </div>
        <div>
          <i class="fa-brands fa-square-github" style="color: #ff9c00;"></i> @Fernandohka
        </div>
        <div>
          <i class="fa-solid fa-envelope" style="color: #ff9c00;"></i>fernando2005andrade@gmail.com
        </div>
      </div>
  
      <div class="d-flex justify-content-center align-items-center">
        <h6>
          ¬©Ô∏è 2024 Copyright | My Movie List
        </h6>
      </div>
    </footer>

    <% console.log("chego no modal") %>

    <!-- Modal -->

    <div class="modal" tabindex="-1" id="atualizarPerfil">
      
      <div class="modal-dialog modal-dialog-centered modal-lg">
        
        <div class="modal-content">

          <div class="modal-header">

            <h5 class="modal-title">Preencha os campos!</h5>

            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>

          </div>

          <div class="modal-body">

            <form action="/updatePerfil/<%= usuario[0].Usuario %>" enctype="multipart/form-data" method="post">

              <div class="container p-3 d-flex justify-content-around align-items-center p-0">
                  <img src="/img/<%= usuario[0].Imagem %>" id="novaImagemPerfil" alt="Imagem do Usu√°rio" onclick="imgClick()">
                  <input class="d-none" type="file" name="newImage" id="newImage" accept="image/*">
              </div>

              <label for="usuarioInput">Usuario</label>
                <input type="text" name="usuarioInput" id="usuarioInput" class="form-control" value="<%= usuario[0].Usuario %>" required>

              <label for="nomeInput">Nome</label>
                <input type="text" name="nomeInput" id="nomeInput" class="form-control" value="<%= usuario[0].Nome %>" required>

              <label for="emailInput">Email</label>
                  <input type="email" name="emailInput" id="emailInput" class="form-control" value="<%= usuario[0].Email %>" required>

              <label for="dtNascInput">Data de Nascimento</label>
                <input type="date" name="dtNascInput" id="dtNascInput" class="form-control" value="<%= usuario[0].DtNasc %>" required>

              <label for="senhaInput">Senha</label>
                <input type="password" name="senhaInput" id="senhaInput" class="form-control" value="<%= usuario[0].Senha %>">

                <br>

              <button type="submit" class="btn btn-primary" id="Atualizar" data-bs-dismiss="modal" aria-label="Close">Atualizar</button>
            </form>

          </div>

        </div>

      </div>
      
    </div>

    <!-- Editar Review Filme -->

    <% for(let i = 0; i < listaFilmes.length; i ++) { %>

      <div class="modal" tabindex="-1" id="editarReviewFilme<%=i%>">
        
        <div class="modal-dialog modal-dialog-centered modal-lg">
          
          <div class="modal-content">
            
          <div class="d-flex justify-content-end p-2">
            
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            
          </div>
          
          <div class="modal-body">
            
            <form action="/updateReview/<%= filmes[i].IDFilme %>/<%= usuario[0].Usuario %>" method="post">
              
              <label for="usuarioInput">Editar Coment√°rio</label>
              <textarea class="form-control" name="novoComentario" rows="2"><%= listaFilmes[i].Comentario %> </textarea>

              <br>
              
              <div class="estrelas">
                
                <input type="radio" id="cm_star-empty" name="novaNota" value="" checked/>
                <label for="cm_star-1"><i class="fa fa-star"></i></label>
                <input type="radio" id="cm_star-1" name="novaNota" value="1" <%= (1 <= listaFilmes[i].Nota) ? 'checked' : "" %>/>
                <label for="cm_star-2"><i class="fa fa-star"></i></label>
                <input type="radio" id="cm_star-2" name="novaNota" value="2" <%= (2 <= listaFilmes[i].Nota) ? 'checked' : "" %>/>
                <label for="cm_star-3"><i class="fa fa-star"></i></label>
                <input type="radio" id="cm_star-3" name="novaNota" value="3" <%= (3 <= listaFilmes[i].Nota) ? 'checked' : "" %>/>
                <label for="cm_star-4"><i class="fa fa-star"></i></label>
                <input type="radio" id="cm_star-4" name="novaNota" value="4" <%= (4 <= listaFilmes[i].Nota) ? 'checked' : "" %>/>
                <label for="cm_star-5"><i class="fa fa-star"></i></label>
                <input type="radio" id="cm_star-5" name="novaNota" value="5" <%= (5 <= listaFilmes[i].Nota) ? 'checked' : "" %>/>
                
              </div>

              <br>
              
              <button type="submit" class="btn btn-primary" id="Atualizar" data-bs-dismiss="modal" aria-label="Close">Atualizar</button>
            </form>
            
          </div>
          
        </div>
        
      </div>
      
    </div>
    
    <div class="modal" tabindex="-1" id="excluirReviewFilme<%=i%>">
      
      <div class="modal-dialog modal-dialog-centered modal-lg">
        
        <div class="modal-content">
          
          <div class="modal-body">
            
            <form action="/deleteReview/<%= filmes[i].IDFilme %>/<%= usuario[0].Usuario %>" enctype="multipart/form-data" method="post">
              
              <h3 class="text-center">Voc√™ est√° excluindo a review do filme <%= filmes[i]['filme.Titulo'] %></h3>
              
              <div class="d-flex justify-content-center">
                <img src="https://cdn-icons-png.flaticon.com/512/272/272238.png" width="150px" height="150px">
              </div>
              
              <p class="text-center">Voc√™ confirma essa exclus√£o?</p>
              
              <div class="d-flex flex-row justify-content-around">
                <button type="submit" class="btnEditReview btn btn-danger" data-bs-dismiss="modal" aria-label="Close">Confirmar</button>
                <button type="button" class="btnEditReview btn btn-success" data-bs-dismiss="modal" aria-label="Close">Cancelar</button>
              </div>
            </form>
            
          </div>
          
        </div>
        
      </div>
      
    </div>
    
    <% } %>

    <!-- Editar Review Serie -->

    <% for(let i = 0; i < listaSeries.length; i ++) { %>

      <div class="modal" tabindex="-1" id="editarReviewSerie<%=i%>">
        
        <div class="modal-dialog modal-dialog-centered modal-lg">
          
          <div class="modal-content">
            
          <div class="d-flex justify-content-end p-2">
            
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            
          </div>
          
          <div class="modal-body">
            
            <form action="/updateReview/<%= series[i].IDSerie %>/<%= usuario[0].Usuario %>" method="post">
              
              <label for="usuarioInput">Editar Coment√°rio</label>
              <textarea class="form-control" name="novoComentario" rows="2"><%= listaSeries[i].Comentario %> </textarea>

              <br>
              
              <div class="estrelas">
                
                <input type="radio" id="cm_star-empty" name="novaNota" value="" checked/>
                <label for="cm_star-1"><i class="fa fa-star"></i></label>
                <input type="radio" id="cm_star-1" name="novaNota" value="1" <%= (1 <= listaSeries[i].Nota) ? 'checked' : "" %>/>
                <label for="cm_star-2"><i class="fa fa-star"></i></label>
                <input type="radio" id="cm_star-2" name="novaNota" value="2" <%= (2 <= listaSeries[i].Nota) ? 'checked' : "" %>/>
                <label for="cm_star-3"><i class="fa fa-star"></i></label>
                <input type="radio" id="cm_star-3" name="novaNota" value="3" <%= (3 <= listaSeries[i].Nota) ? 'checked' : "" %>/>
                <label for="cm_star-4"><i class="fa fa-star"></i></label>
                <input type="radio" id="cm_star-4" name="novaNota" value="4" <%= (4 <= listaSeries[i].Nota) ? 'checked' : "" %>/>
                <label for="cm_star-5"><i class="fa fa-star"></i></label>
                <input type="radio" id="cm_star-5" name="novaNota" value="5" <%= (5 <= listaSeries[i].Nota) ? 'checked' : "" %>/>
                
              </div>

              <br>
              
              <button type="submit" class="btn btn-primary" id="Atualizar" data-bs-dismiss="modal" aria-label="Close">Atualizar</button>
            </form>
            
          </div>
          
        </div>
        
      </div>
      
    </div>
    
    <div class="modal" tabindex="-1" id="excluirReviewSerie<%=i%>">
      
      <div class="modal-dialog modal-dialog-centered modal-lg">
        
        <div class="modal-content">
          
          <div class="modal-body">
            
            <form action="/deleteReview/<%= series[i].IDSerie %>/<%= usuario[0].Usuario %>" enctype="multipart/form-data" method="post">
              
              <h3 class="text-center">Voc√™ est√° excluindo a review da serie <%= series[i]['serie.Titulo'] %></h3>
              
              <div class="d-flex justify-content-center">
                <img src="https://cdn-icons-png.flaticon.com/512/272/272238.png" width="150px" height="150px">
              </div>
              
              <p class="text-center">Voc√™ confirma essa exclus√£o?</p>
              
              <div class="d-flex flex-row justify-content-around">
                <button type="submit" class="btnEditReview btn btn-danger" data-bs-dismiss="modal" aria-label="Close">Confirmar</button>
                <button type="button" class="btnEditReview btn btn-success" data-bs-dismiss="modal" aria-label="Close">Cancelar</button>
              </div>
            </form>
            
          </div>
          
        </div>
        
      </div>
      
    </div>
    
    <% } %>

    <!-- aviso review excluida -->
    
    <div class="modal" tabindex="-1" id="reviewExcluida">
      
      <div class="modal-dialog modal-dialog-centered modal-sm">
        
        <div class="modal-content">
          
          <div class="modal-header">
            
            <h5 class="modal-title"></h5>
            
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close" id="closeModal"></button>
            
          </div>
          
          <div class="modal-body">
            
            <p class="text-center">Review exclu√≠da com sucesso!</p>
            
          </div>
          
        </div>
        
      </div>
      
    </div>

    <!-- Modal de logoff -->

    <div class="modal" tabindex="-1" id="logout">
      
